<!-- profile page -->
<div class="jumbotron center">
<% if user_signed_in? %>
<h2> <%= current_user.name %> </h2>
<% else %> 
<h2> Hello TasteBud </h2>
<% end %> 
<%= link_to 'Edit Profile', edit_user_registration_path %>
</div>

<h1>Your Lists</h1>

  <body>
    <% @lists.each do |list| %>
      <br>
       <%= link_to list.title, table_path(list)%> 
        <br>
        <%= link_to 'Edit', edit_table_path(list) %>
        <br>
        <%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %>
        <br>
    <% end %>
  </body>


<br>

<%= link_to 'New List', new_table_path %>


